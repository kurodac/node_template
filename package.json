{
  "engines": {
    "node": "15.*.*"
  },
  "name": "node-template",
  "version": "0.0.1",
  "description": "This is a template of node project.",
  "main": "index.js",
  "scripts": {
    "test": "npx jest --coverage",
    "build": "webpack --config webpack.prod.js",
    "start": "npx webpack-dev-server --config webpack.dev.js",
    "start:prod": "npx webpack-dev-server --config webpack.prod.js",
    "jsdoc": "npx documentation build -f md src/main.js | npx add-text-to-markdown README.md --section \"Usage\" --write"
  },
  "prettier": "prettier-airbnb-config",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:kurodac/node_template.git"
  },
  "author": "Eiji Kuroda",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "add-text-to-markdown": "^2.0.0",
    "babel-jest": "^26.6.3",
    "clean-webpack-plugin": "^3.0.0",
    "documentation": "^13.1.0",
    "eslint": "^7.15.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^1.18.2",
    "prettier-airbnb-config": "^1.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  }
}